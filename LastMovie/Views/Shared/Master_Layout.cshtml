@using LastMovie.Controllers
@using LastMovie.Models;

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>AdminLTE 3 | Dashboard 3</title>

	<!-- Google Font: Source Sans Pro -->
	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
	<!-- Font Awesome Icons -->
	<link rel="stylesheet" href="~/AdminTLE/plugins/fontawesome-free/css/all.min.css">
	<!-- IonIcons -->
	<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
	<!-- Theme style -->
	<link rel="stylesheet" href="~/AdminTLE/dist/css/adminlte.min.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css"
		  integrity="sha512-YWzhKL2whUzgiheMoBFwW8CKV4qpHQAEuvilg9FAn5VJUDwKZZxkJNuGM4XkWuk94WCrrwslk8yWNGmY1EduTA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body class="hold-transition sidebar-mini">
	<div class="wrapper">
		<!-- Navbar -->
		<nav class="main-header navbar navbar-expand navbar-dark">
			<!-- Left navbar links -->
			<ul class="navbar-nav">
				<li class="nav-item">
					<a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
				</li>
				<li class="nav-item d-none d-sm-inline-block">
					<a href="~/Loading/Loading_Master_Home" class="nav-link">Home</a>
				</li>
			</ul>

		</nav>
		<!-- /.navbar -->
		<!-- Main Sidebar Container -->
		<aside class="main-sidebar sidebar-dark-primary elevation-4">
			<!-- Brand Logo -->
			<a href="~/Loading/Loading_Master_Home" class="brand-link">
				<img src="~/Image/Logo.png" alt="Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
				<span class="brand-text font-weight-light">Netflix</span>
			</a>

			<!-- Sidebar -->
			<div class="sidebar">

				<!-- Sidebar Menu -->
				<nav class="mt-2">
					<ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
						<!-- Add icons to the links using the .nav-icon class
							 with font-awesome or any other icon font library -->
						<li class="nav-item menu-open">
							<a href="#" class="nav-link active">
								<i class="nav-icon fa-solid fa-house"></i>
								<p>
									Home
									<i class="right fas fa-angle-left"></i>
								</p>
							</a>
							<ul class="nav nav-treeview">
								<li class="nav-item HomePage">
									<a href="~/Loading/Loading_Master_Home" class="nav-link">
										<i class="far fa-circle nav-icon"></i>
										<p>Home</p>
									</a>
								</li>
							</ul>
						</li>


						<li class="nav-header">Other</li>
						<li class="nav-item mymessage">
							<a href="~/Loading/Loading_Master_Message" class="nav-link">
								<i class="nav-icon fa-solid fa-envelope"></i>
								<p>
									Message
								</p>
							</a>
						</li>
						<li class="nav-item mycomment">
							<a href="~/Loading/Loading_Master_Comment" class="nav-link">
								<i class="nav-icon fa-solid fa-comments"></i>
								<p>
									Comment
								</p>
							</a>
						</li>
						<li class="nav-item mypro">
							<a href="~/Loading/Loading_Master_MyProduct" class="nav-link">

								<i class="nav-icon fa-brands fa-product-hunt"></i>
								<p>
									My Product
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>
						<li class="nav-item sellerpro">
							<a href="~/Loading/Loading_Master_Seller_Product" class="nav-link">

								<i class="nav-icon fa-solid fa-truck-moving"></i>
								<p>
									Seller Product
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>
						<li class="nav-item newproductpro">
							<a href="~/Loading/Loading_Master_New_Movie" class="nav-link">

								<i class="nav-icon fa-solid fa-plus"></i>
								<p>
									New Movie
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>
						<li class="nav-item userpro">
							<a href="~/Loading/Loading_Master_User" class="nav-link">

								<i class="nav-icon fa-solid fa-user"></i>
								<p>
									User
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>
						<li class="nav-item sellerpro2">
							<a href="~/Loading/Loading_Master_Seller" class="nav-link">

								<i class="nav-icon fa-solid fa-user-tie"></i>
								<p>
									Seller
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>
						<li class="nav-item analysis">
							<a href="~/Loading/Loading_Master_Analysis" class="nav-link">
								<i class="nav-icon fa-solid fa-chart-line"></i>
								<p>
									Analysis
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>
						<li class="nav-item allmessage">
							<a href="~/Loading/Loading_Master_All_Message" class="nav-link">
								<i class="nav-icon fa-solid fa-message"></i>
								<p>
									All Message
									<span class="badge badge-info right">@ViewBag.TrCount</span>
								</p>
							</a>
						</li>


						<li class="nav-header">Login</li>
						<li class="nav-item">
							<a href="~/Loading/Loading_Sign_Out" class="nav-link">
								<i class="nav-icon fa-solid fa-right-to-bracket"></i>
								<p>Sign Out</p>
							</a>
						</li>



						

					</ul>
				</nav>
				<!-- /.sidebar-menu -->
			</div>
			<!-- /.sidebar -->
		</aside>

		<div class="content-wrapper">
			<div>
				@RenderBody()
			</div>
			<!-- /.content -->
		</div>
		<!-- /.content-wrapper -->
		<!-- Control Sidebar -->
		<aside class="control-sidebar control-sidebar-dark">
			<!-- Control sidebar content goes here -->
		</aside>
		<!-- /.control-sidebar -->
		<!-- Main Footer -->

	</div>
	<!-- ./wrapper -->
	<!-- REQUIRED SCRIPTS -->
	<!-- jQuery -->
	<script src="~/AdminTLE/plugins/jquery/jquery.min.js"></script>
	<!-- Bootstrap -->
	<script src="~/AdminTLE/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
	<!-- AdminLTE -->
	<script src="~/AdminTLE/dist/js/adminlte.js"></script>

	<!-- OPTIONAL SCRIPTS -->
	@*	<script src="~/AdminTLE/plugins/chart.js/Chart.min.js"></script>*@
	<!-- AdminLTE for demo purposes -->
	@*<script src="~/AdminTLE/dist/js/demo.js"></script>*@
	<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
	<script src="~/AdminTLE/dist/js/pages/dashboard3.js"></script>
</body>
</html>
