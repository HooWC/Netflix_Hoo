			<ul class="pagination_cc modal_cc">
				<li><a href="#" class="prev_cc" id="prev">Previous</a></li>
				<div id="cv">
				</div>
				<li><a href="#" class="next_cc" id="next">Next</a></li>
			</ul>
		</div>


	</div>


</div>




<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>


	$(".mypro").css({
		"background":"red",
		"border-radius":"5px"
	});

	$(function(){

		$("#mySearchProduct").keyup(function(){

			$.ajax({

				type:"POST",
				url:"/Ajax/GetProduct_List_Search_Master",
				dataType:"json",
				data:{
					Search:$(this).val()
				},
				success:function(data){
					$("#mypro").empty();
                    $("#cv").empty();
					GetMovie(data,"mypro")
					var rowShown = 8;
					$(".container_cc").css("display","block");
					var rowsTotal = $("#mypro .show").length;
					var rowShown = 8;
					var numPages = rowsTotal / rowShown;
					$("#mypro .show").slice(0,rowShown).show();

					for(var i = 0 ; i < numPages;i++){
						var pageNum = i+1;
						$("#cv").append(`<li><a class="back" rel='${i}'>${pageNum}</a></li>`)
						$(".back").first().css("background","red");
					}

					$("#cv a").click(function(){
						$(".back").css("background","#468599");
						$(this).css("background","red");

						var currPage = $(this).attr("rel")
						page = currPage

						var startItem = currPage * rowShown;
						var endItem = startItem + rowShown;
						var index = $(this).parent().index();

						$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","block").animate({opacity:1},300)

					})

					$("#prev").click(function(){

						if(page != 0){
							var num = page - 1;
							page = num;
							var startItem = num * rowShown;
							var endItem = startItem + rowShown;

							$(".back").css("background","#468599");
							$(".back").eq(page).css("background","red");

							$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","block").animate({opacity:1},300)
							//$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","table-row").animate({opacity:1},300)
						}

					})

					$("#next").click(function(){
						var n = $(".back").last().attr("rel");

						if(page != n){
							var num1 = parseInt(page) + 1;
							page = num1;
							var startItem = num1 * rowShown;
							var endItem = startItem + rowShown;

							$(".back").css("background","#468599");
							$(".back").eq(page).css("background","red");

							$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","block").animate({opacity:1},300)
						}

					})
				}
			})
		})

		var page = 0;

		$.ajax({
			type:"GET",
			url:"/Ajax/Get_Master_Movie",
			dataType:"json",
			success:function(data){
				GetMovie(data,"mypro")
				$(".container_cc").css("display","block");
				var rowsTotal = $("#mypro .show").length;
				var rowShown = 8;
				var numPages = rowsTotal / rowShown;
				$("#mypro .show").slice(0,rowShown).show();


				for(var i = 0 ; i < numPages;i++){
					var pageNum = i+1;
					$("#cv").append(`<li><a class="back" rel='${i}'>${pageNum}</a></li>`)
					$(".back").first().css("background","red");
				}

				$("#cv a").click(function(){
					$(".back").css("background","#468599");
					$(this).css("background","red");

					var currPage = $(this).attr("rel")
					page = currPage

					var startItem = currPage * rowShown;
					var endItem = startItem + rowShown;
					var index = $(this).parent().index();

					$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","block").animate({opacity:1},300)

				})

				$("#prev").click(function(){

					if(page != 0){
						var num = page - 1;
						page = num;
						var startItem = num * rowShown;
						var endItem = startItem + rowShown;

						$(".back").css("background","#468599");
						$(".back").eq(page).css("background","red");

						$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","block").animate({opacity:1},300)
						//$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","table-row").animate({opacity:1},300)
					}

				})

				$("#next").click(function(){
					var n = $(".back").last().attr("rel");

					if(page != n){
						var num1 = parseInt(page) + 1;
						page = num1;
						var startItem = num1 * rowShown;
						var endItem = startItem + rowShown;

						$(".back").css("background","#468599");
						$(".back").eq(page).css("background","red");

						$("#mypro .show").css("opacity","0").hide().slice(startItem,endItem).css("display","block").animate({opacity:1},300)
					}

				})

			}//success
		})//ajax



	})

